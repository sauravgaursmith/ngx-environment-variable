{
  "name": "ngx-environment-variable",
  "version": "0.0.1",
  "description": "An angular x script to set environment variables",
  "main": "dist/ngx-environment-variable.umd.js",
  "jsnext:main": "dist/ngx-environment-variable.esm.js",
  "module": "dist/ngx-environment-variable.esm.js",
  "types": "dist/ngx-environment-variable.d.ts",
  "scripts": {
    "bundle": "rimraf dist && rollup -c rollup.config.umd.js && rollup -c rollup.config.esm.js && tsc",
    "postbundle": "node tools/removecode.js",
    "build": "npm run bundle"
  },
  "keywords": [
    "angular",
    "environment",
    ".env",
    "variable",
    "ngx",
    "process",
    "env",
    "2/4/5/6"
  ],
  "author": "Saurav Singh",
  "license": "MIT",
  "devDependencies": {
    "del": "^3.0.0",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-typescript": "^0.8.1"
  }
}
